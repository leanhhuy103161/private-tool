{
  "compilerOptions": {
    "rootDir": ".",
    "esModuleInterop": true,
    "baseUrl": "src",
    "paths": {
      "*": ["*"]
    },
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "preserve",
    "isolatedModules": true,
    "strictNullChecks": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/modules/home/stores/slices/flow.js", "src/modules/home/stores/slices/flow.js", "src/modules/home/components/constant.js", "src/modules/home/components/constant.js"],
  "exclude": ["node_modules", "specifications"]
}
